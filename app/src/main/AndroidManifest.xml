<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aabb.airlessbreathalyzer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".new_profile"
            android:label="@string/title_activity_profiles"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".pick_profile"
            android:label="@string/title_activity_pick_profile"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mathTest"
            android:label="@string/title_activity_math_test"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".reflexTest"
            android:label="@string/title_activity_reflex_test"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".shortTermMemoryTestStart"
            android:label="@string/title_activity_short_term_memory_test_start"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".shortTermMemoryTestFinish"
            android:label="@string/title_activity_short_term_memory_test_finish"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>

<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.aabb.airlessbreathalyzer.analyzeData">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/finalScore"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="56dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/race"
        android:layout_marginTop="76dp"
        android:layout_below="@+id/finalScore"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/sex"
        android:layout_alignTop="@+id/weight"
        android:layout_toRightOf="@+id/finalScore"
        android:layout_toEndOf="@+id/finalScore" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/weight"
        android:layout_alignTop="@+id/race"
        android:layout_toLeftOf="@+id/finalScore"
        android:layout_toStartOf="@+id/finalScore" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/reflex"
        android:layout_alignTop="@+id/mem"
        android:layout_alignLeft="@+id/race"
        android:layout_alignStart="@+id/race" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/math"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/finalScore"
        android:layout_alignStart="@+id/finalScore" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/mem"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/sex"
        android:layout_alignStart="@+id/sex" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/age"
        android:layout_alignTop="@+id/sex"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
